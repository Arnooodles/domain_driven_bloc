{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "[Debug] Launch development",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--flavor",
        "development",
        "--target",
        "lib/main.dart"
      ]
    },
    {
      "name": "[Profile] Launch development",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "program": "lib/main.dart",
      "args": [
        "--flavor",
        "development",
        "--target",
        "lib/main.dart",
      ]
    },
    {
      "name": "[Debug] Launch staging",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": ["--flavor", "staging", "--target", "lib/main.dart"]
    },
    {
      "name": "[Debug] Launch production",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": ["--flavor", "production", "--target", "lib/main.dart"]
    },
    {
      "name": "[Web][Debug] Launch development",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": ["--dart-define", "flavor=development", "--target", "lib/main.dart"]
    },
    {
      "name": "[Web][Debug] Launch staging",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": ["--dart-define", "flavor=staging", "--target", "lib/main.dart"]
    },
    {
      "name": "[Web][Debug] Launch production",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": ["--dart-define", "flavor=production", "--target", "lib/main.dart"]
    },
    {
      "name": "Golden",
      "request": "launch",
      "type": "dart",
      "codeLens": {
        "for": ["run-test", "run-test-file"]
      },
      "args": ["--update-goldens"]
    }
  ]
}
